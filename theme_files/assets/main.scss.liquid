$HeroHeader--min_height_sm: 556px;
$HeroHeader--min_height_md: 556px;
$HeroHeader--min_height_lg: 556px;

$HeroHeader--thin--min_height: 330px;
$HeroHeader--thin--min_height_sm: 330px;
$HeroHeader--thin--min_height_md: 330px;
$HeroHeader--thin--min_height_lg: 330px;

$HeroHeader--breakpoint: $Viewport-sm;

.HeroHeader {
    @include make-Padding();
    padding-top: 15px;
    padding-bottom: 15px;

    display: table;
    width: 100%;

    background-size: cover;
    background-position: center;
    text-align: center;


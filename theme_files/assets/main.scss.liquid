@mixin make-Offcanvas--width() {
    width: $Offcanvas--width;

    @media (min-width: $Viewport-xs) {
        width: $Offcanvas--width_xs;
    }

    @media (min-width: $Viewport-sm) {
        width: $Offcanvas--width_sm;
    }
}

@mixin make-Offcanvas--transform($factor: 1.0) {
    transform: translateX(($Offcanvas--width * $factor));

    @media (min-width: $Viewport-xs) {
        transform: translateX(($Offcanvas--width_xs * $factor));
    }

    @media (min-width: $Viewport-sm) {
        transform: translateX(($Offcanvas--width_sm * $factor));
    }
}

@mixin make-Offcanvas--revert() {
    &.Offcanvas {
        position: static;
        height: auto;
        width: auto;
        border-right-width: 0;
        transition: none;
        transform: none;

        z-index: auto;
    }

    html.no-csstransforms &.Offcanvas {
        transition: none;
        left: auto;
    }

    > .Offcanvas-scroller {
        background: transparent;
        height: auto;

        overflow-y: visible;
        overflow-x: visible;
    }
}

@mixin make-Offcanvas--reveal() {
    max-height: 0;
    overflow: hidden;

    &.is-Offcanvas--open {
        max-height: 400px;
        max-height: 80vh;
    }
}

.Offcanvas--reveal {
    @include make-Offcanvas--reveal();
}

@mixin make-Offcanvas--auxiliary() {
    position: fixed;
    height: 100%;

    border-right: $Offcanvas--border_width solid $Offcanvas--border_color;

    @include make-Offcanvas--width();

    transition: transform 0.5s;

    &.is-Offcanvas--open {
        @include make-Offcanvas--transform(0.0);
    }

    &.is-Offcanvas--closed {
        @include make-Offcanvas--transform(2.2);
    }
}

@mixin make-Offcanvas--auxiliary_lv1() {
    z-index: $Offcanvas--lv1--z_index;

    &.is-Offcanvas--open {
        @include make-Offcanvas--transform(0.0);
    }

    &.is-Offcanvas--depth_1 {
        @include make-Offcanvas--transform(-0.2);
    }

    &.is-Offcanvas--depth_2 {
        @include make-Offcanvas--transform(-0.4);
    }

    &.is-Offcanvas--depth_3 {
        @include make-Offcanvas--transform(-0.6);
    }

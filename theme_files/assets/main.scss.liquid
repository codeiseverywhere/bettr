@mixin make-List--revert() {
    list-style: none inside none;
    margin: 0;
    padding: 0;
}

ul {
    @include make-List--revert();
}

ol {
    @include make-List--revert();
}

blockquote {
    position: relative;
    text-align: center;

    margin: $Blockquote--margin_vertical $Blockquote--margin_horizontal;

    &:before {
        content: '"';
        z-index: $ZIndex-backdrop_type;
        position: absolute;
        left: 0;
        top: 0;

        font-size: $Blockquote--quote_size;
        margin-top: ($Blockquote--quote_size * -0.5);
        color: $Blockquote--quote_color;
    }

    &:after {
        content: '"';
        z-index: $ZIndex-backdrop_type;
        position: absolute;
        right: 0;
        bottom: 0;

        font-size: $Blockquote--quote_size;
        margin-bottom: ($Blockquote--quote_size * -1.25);
        color: $Blockquote--quote_color;
    }
}

@mixin make-List($_) {
    @include make-Paragraph();

    @if $_ == ul {
        list-style: disc;
    } @else if $_ == ol {
        list-style: decimal;
    }

    list-style-position: outside;
    margin: $List--spacing 0;
    padding: 0 0 0 $List--indent;

    //Override any internal paragraphs
    p {
        margin: 0;
    }

    //2nd level lists
    li ul {
        list-style: lower-latin;
    }

    li ol {
        list-style: circle;
    }

    //3rd level lists
    li li ul {
        list-style: lower-roman;
    }

    li li ol {
        list-stule: square;
    }
}

//Re-enable list and other styling reverted by default.
@mixin make-Wysiwyg() {
    font-family: $Paragraph-font;
    font-size: $Paragraph--font_size;
    line-height: $Paragraph--line_height;
    font-weight: $Paragraph-weight;
    font-style: $Paragraph-style;
    -webkit-font-smoothing: antialiased;

    @media (min-width: $Viewport-sm) {
        font-size: $Paragraph--font_size_sm;
        line-height: $Paragraph--line_height_sm;
    }

    ul {
        @include make-List(ul);
    }

    ol {
        @include make-List(ol);
    }
}






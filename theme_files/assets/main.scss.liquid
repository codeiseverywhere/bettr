// Custom SASS
.SiteHeader {
  .SiteHeader-navigation {    
    .NavMenu--main_menu {
      padding-left: 0;
      .NavMenu--main_menu-link {
        a {
          span {
            font-size: 12px;
            text-transform: uppercase;
            letter-spacing: 1px;
          }
          &:hover {
            span {
              text-decoration: underline;
            }
          }
        } 
      }
    }
    .SiteHeader-Icon {
      padding-left: 0;
      img {
        max-height: 27px;
      }
      &:last-child {
        padding-right: 0;
      }
    }
    .SiteHeader-search {
      width: auto;
    }
    &.SiteHeader-navigation--third_menu {
      height: 45px;
      .NavMenu-link {
        margin-right: 65px;
        a {
          span {
            padding-right: 0;
          }
        }
        &:last-child {
          a {
            span {
              padding-right: 0;
              padding-left: 0;
            }
          }
        }
      }
    }
  }
  .searchbar {
    padding: 0 8%;
    clear: both;
    background-color: $white;
    box-shadow: 0 0 10px 0 rgba(0,0,0,0.5);
    .searchbar-inner {
      position: relative;
      display: flex;
      align-items: center;
      overflow: hidden;
      height: 0px;
      @extend .ease03s;
      > img {
        max-width: 20px;
      }
      form {
        width: 100%;
        input[type="text"] {
          padding: 0 0 0 15px;
          border: none;
          line-height: 40px;
          width: 100%;
          outline: none;
          font-size: 12px;
          font-family: $defaultFam;
          font-weight: 500;
          letter-spacing: 0.5px;
          line-height: 16px;
        }
        button {
          position: absolute;
          left: 0;
          top: 0;
          cursor: pointer;
          z-index: 10;
          background: transparent;
          border: none;
          width: 20px;
          height: 40px;
          padding: 0;
        }
      }
    }
    &.search-open {
      .searchbar-inner {
        height: 40px;
      }
    }
  }
}

@media (min-width: 1200px) {

  .NavMenu--main_menu {
    padding-left: 0;
    .NavMenu--main_menu-link {
      margin-right: 45px;
      a, a:hover, a:focus {
        span {
          padding-right: 0px;
          padding-left: 0;
        }
      } 
    }
  }

}


.SiteHeader-form_button_wrapper {
    position: absolute;
    right: $SiteHeader-form_item--padding_horizontal;
    top: 50%;
    transform: translateY(-50%);

    width: auto;
    margin: 0;
}

.SiteHeader-form_button {
    @include make-Button--revert(button);

    height: $FormItem--height;
    min-width: $FormItem--height;
    margin: $SiteHeader-form_item--margin_vertical 0;

    display: inline-block;
    vertical-align: middle;

    color: $SiteHeader-form_item--color;

    //Assuming that if the button has an icon, it's installed into :before
    &:before {
        display: inline-block;
        vertical-align: middle;
    }
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-form_item {
        margin: 0;

        padding-left: 0px;
        padding-right: 0px;

        height: 100%;

        &:before {
            content: "";
            height: 100%;
            display: inline-block;
            vertical-align: middle;
        }

        > * {
            display: inline-block;
            vertical-align: middle;
        }

        input[type="search"] {
            float: none;
        }
    }
}



$SiteHeader-notice_bar--height: {{ settings.announcement-bar-height }};


$SiteHeader-notice_bar--bg_color: $Color--Announcement-Bar-background;
$SiteHeader-notice_bar--color: $Color--Announcement-Bar-text;

$SiteHeader-notice_bar--font_size: 11px;
$SiteHeader-notice_bar--font_weight: $Font-weight--bold;
$SiteHeader-notice_bar--line_height: 1.25;

$SiteHeader-notice_bar--line_height_calculated: ($SiteHeader-notice_bar--font_size * $SiteHeader-notice_bar--line_height);
$SiteHeader-notice_bar--margin_vertical: (($SiteHeader-notice_bar--height_bp - $SiteHeader-notice_bar--line_height_calculated) / 2);

.SiteHeader-notice_bar {
    // position: absolute;
    // top: 0;
    // left: 0;
    // right: 0;

    width: 100%;
    height: $SiteHeader-notice_bar--height;

    font-family: $Font-family;
    font-size: $SiteHeader-notice_bar--font_size;
    font-weight: $SiteHeader-notice_bar--font_weight;
    line-height: $SiteHeader-notice_bar--line_height;

    background-color: $SiteHeader-notice_bar--bg_color;
    color: $SiteHeader-notice_bar--color
}

.SiteHeader-notice {
    // @include make-Container($SiteHeader--breakpoint_sm, $Container-sm);
    // @include make-Container($SiteHeader--breakpoint_md, $Container-md);
    // @include make-Container($SiteHeader--breakpoint_lg, $Container-lg);
    text-align: center;
    margin: 0px 0px;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;

    color: $SiteHeader-notice_bar--color;

    @extend .clearfix;
}

@media (max-width: $SiteHeader--breakpoint_max) {
    .SiteHeader-notice {
        display: none;
    }
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-notice_bar {
        height: $SiteHeader-notice_bar--height_bp;
    }
}

.SiteHeader-notice-text {
    margin: 0px auto;
    font-size: 12px
}

$SiteHeader-account_button--icon_size: 24px;

$SiteHeader-account_button-padding_vertical: (($SiteHeader--height - $SiteHeader-account_button--icon_size) / 2);
$SiteHeader-account_button-padding_horizontal: 8px;

$SiteHeader-account_button-padding_vertical_bp: (($SiteHeader--content_row_height_bp - $SiteHeader-account_button--icon_size) / 2);
$SiteHeader-account_button-padding_horizontal_bp: 8px;

.SiteHeader-account {
    position: relative;
}

.SiteHeader-account_button {
    @include make-Button--revert(button);
    @include make-Icon--before($Icon-user, $SiteHeader-account_button--icon_size);
    
    padding: $SiteHeader-account_button-padding_vertical $SiteHeader-account_button-padding_horizontal;
    
    color: inherit;
    
    &:before {
        display: block;
    }
}

.SiteHeader-account_menu {
    @extend .SiteHeader-dropdown;
}

.SiteHeader-account {
    float: left;
    padding-left: 10px;
}

.SiteHeader-account_button {
}

.SiteHeader-block--main_menu {
    float: left;
    width: 100%;
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-account_button {
        padding: $SiteHeader-account_button-padding_vertical_bp $SiteHeader-account_button-padding_horizontal_bp;
    }
    .SiteHeader-block--main_menu {
        float: right;
        width: auto;
    }
    .SiteHeader-account {
        padding-left: 0;
    }
}

.SiteHeader-dropdown {
    @media (max-width: $SiteHeader--breakpoint_max) {
        @include make-Offcanvas--revert();

        &, &.Offcanvas {
            &.is-Offcanvas--closed {
                display: none;
            }

            &.is-Offcanvas--open {
                display: block;
            }
        }
    }

    @media (min-width: $SiteHeader--breakpoint) {
        @include make-Offcanvas--revert();

        &, &.Offcanvas {
            position: absolute;
            top: 100%;
            left: 0;

            background-color: $SiteHeader--background;
            transition: opacity 0.15s;
            transform-origin: top;

            &.is-Offcanvas--closed {
                transform: scaleY(0);
                opacity: 0;
                transition: opacity 0.15s, transform 0s 0.15s;
            }

            &.is-Offcanvas--open {
                transform: scaleY(1);
                opacity: 1;
            }
        }
    }
}

$SiteHeader-cart_button--icon_size: 24px;

$SiteHeader-cart-item-count--bg_color: $Color--red;
$SiteHeader-cart-item-count--color: contrast($SiteHeader-cart-item-count--bg_color, $Font-color--black, $Font-color--white);
$SiteHeader-cart_item_count--size: 20px;
$SiteHeader-cart_item_count--font_size: 13px;
$SiteHeader-cart_item_count--line_height: 1.0;

$SiteHeader-cart_item_count--padding: (($SiteHeader-cart_item_count--size - $SiteHeader-cart_item_count--font_size * $SiteHeader-cart_item_count--line_height) / 2);
$SiteHeader-cart_item_count--border_radius: ($SiteHeader-cart_item_count--size / 2);

$SiteHeader-cart_button-padding_vertical_bp: (($SiteHeader--content_row_height_bp - $SiteHeader-cart_button--icon_size) / 2);
$SiteHeader-cart_button-padding_horizontal_bp: 8px;

.SiteHeader-cart {
    position: relative;
    display: flex;
    align-items: center;
    height: 100%;
}

.SiteHeader-cart_notification {
    width: 14px;
    height: 14px;
    border-radius: 10px;

    position: absolute;
    right: -5px;
    top: 42%;
    transform: translateY(-58%);

    font-size: 10px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.SiteHeader-cart_button {
    @include make-Button--revert(a);
    display: block;

    &:after {
        display: block;
    }
}

.SiteHeader-cart_icon {
    height: 100%;    
    min-width: 29px;
    min-height: 27px;
}

.SiteHeader-cart_menu {
    font-family: $Font-family;
}

.SiteHeader-cart_item_count {
    background-color: $SiteHeader-cart-item-count--bg_color;
    color: $SiteHeader-cart-item-count--color;

    min-width: $SiteHeader-cart_item_count--size;
    height: $SiteHeader-cart_item_count--size;

    font-size: $SiteHeader-cart_item_count--font_size;
    line-height: $SiteHeader-cart_item_count--line_height;
    padding: $SiteHeader-cart_item_count--padding;

    border-radius: 100%;

    text-align: center;
    float: left;
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-cart {
        float: left;
    }

    .SiteHeader-cart_button {
        padding: $SiteHeader-cart_button-padding_vertical_bp $SiteHeader-cart_button-padding_horizontal_bp;
        height: $SiteHeader--content_row_height_bp;

        &:after {
            float: left;
        }
    }

    .SiteHeader-cart_menu {
        position: absolute;
        top: 100%;

        box-shadow: $SiteHeader--box_shadow;
    }

    .SiteHeader-cart_menu--placed_on_left {
        left: 0;
    }

    .SiteHeader-cart_menu--placed_on_right {
        right: 0;
    }
}

$SiteHeader--extra_row--height: {{ settings.siteHeader-bottom-row-height }};

$SiteHeader--extra_row--border_top_width: $SiteHeader--border_width;
$SiteHeader--extra_row--border_top_color: $SiteHeader--border_color;

.SiteHeader-navigation--third_menu {
    clear: left;
    float: left;

    width: 100%;
    height: $SiteHeader--extra_row--height;

    background-color: $Color--SiteHeader__Bottom-Background;
    color: $Color--SiteHeader__Bottom;

    &:before {
        content: "";
        border-top: 1px solid $SiteHeader--extra_row--border_top_color;
        position: absolute;
        left: 0;
        right: 0;
    }
}

.SiteHeader-navigation--third_menu div ul li {
    cursor: pointer;
}
.SiteHeader-navigation--third_menu div ul li a {
    color: $Color--SiteHeader__Bottom;
    font-size: 12px;
}
.SiteHeader-navigation--third_menu div ul li a span {
    padding: 10px 25px;
}

$SiteHeader-search--border_radius: 20px;

.SiteHeader-search {
    position: relative;

    input[type='search'] {
        border-radius: $SiteHeader-search--border_radius;
    }
}

.SiteHeader-search_button {
    @include make-Icon--before($Icon-search);
    margin: 0;
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-search {
        height: $SiteHeader--content_row_height_bp;
    }

    .SiteHeader-search--first_menu {
        float: right;
    }

    .SiteHeader-search--second_menu {
        float: left;
    }
}

$SiteHeader-contact--breakpoint: $Viewport-xs;

.SiteHeader-contact_toast {
    @include make-Offcanvas--toast();

    z-index: $ZIndex-modal;

    text-align: center;
}

.SiteHeader-contact_content {
    display: table;
    width: 100%;
}

.SiteHeader-contact_information {
    @include make-Padding();
    padding-top: 50px;
    padding-bottom: 50px;

    background-size: cover;
    background-position: bottom center;
    background-repeat: no-repeat;

    @media (min-width: $SiteHeader-contact--breakpoint) {
        display: table-cell;
        width: 50%;
    }
}

.SiteHeader-contact_form_wrapper {
    padding-top: 50px;
    padding-bottom: 50px;

    background-size: cover;
    background-position: bottom center;
    background-repeat: no-repeat;

    @media (min-width: $SiteHeader-contact--breakpoint) {
        display: table-cell;
        width: 50%;
    }
}

.SiteHeader-contact_title {
    margin-top: 0;
}

.SiteHeader-contact_form_title {
    @include make-Padding();
    margin-top: 0;
}


.SiteHeader {
    position: fixed;
    top: 0;
    width: 100%;

    margin: 0;

    background: $SiteHeader--background;
    box-shadow: $SiteHeader--box_shadow;

    z-index: $SiteHeader--z_index;

    transition: margin 0.5s;

    z-index: 1000;
}

.SiteHeader--fixed {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
}

.SiteHeader--flush {
    max-width: none;
    margin: 0;

    &.is-SiteHeader--at_top {
        margin: 0;
    }
}

.SiteHeader--detached {
    &.is-SiteHeader--at_top {
        margin: $SiteHeader--margin;
    }

    @media (min-width: ($Viewport-lg * 1.1)) {
        max-width: none;
        margin: 0 auto;

        &.is-SiteHeader--at_top {
            margin: $SiteHeader--margin 2%;
        }
    }
}

.SiteHeader-spacing {
    height: $SiteHeader--height;
}

.SiteHeader-container {
    @include make-Container($SiteHeader--breakpoint_sm, $Container-sm);
    @include make-Container($SiteHeader--breakpoint_md, $Container-md);
    @include make-Container($SiteHeader--breakpoint_lg, $Container-lg);
    margin: 0 auto;

    @extend .clearfix;

    max-width: 100% !important;
}

.SiteHeader-overlap {
    padding-top: $SiteHeader--height;

    @media (min-width: $SiteHeader--breakpoint) {
        padding-top: $SiteHeader--height_bp;
    }
}

@media (max-width: $SiteHeader--breakpoint_max) {
    .SiteHeader-content {
        @include make-Border(border-bottom, $SiteHeader--border_width, $SiteHeader--border_color);
    }
}

@media (min-width: $SiteHeader--breakpoint) {
    .SiteHeader-spacing {
        height: $SiteHeader--height_bp;
    }

    .SiteHeader-content {
        order: 1;
    }
}

$AjaxCart--background_color: $Color--white;

$AjaxCart--width: 320px;

$AjaxCart--gutter_width: $Grid--gutter_width;

$AjaxCart-cart_header--spacing: 5px;

$AjaxCart-cart_title--font_size: 20px;
$AjaxCart-cart_title--line_height: 1.2;

$AjaxCart-dismiss--size: 14px;
$AjaxCart-dismiss--misalignment: (($AjaxCart-cart_title--font_size * $AjaxCart-cart_title--line_height) - $AjaxCart-dismiss--size) / 2;
$AjaxCart-dismiss--color: contrast($AjaxCart--background_color, $Font-color--black, $Font-color--white);

.AjaxCart {
    @extend .clearfix;

    background-color: $AjaxCart--background_color;

    width: 100%;

    @media (min-width: $SiteHeader--breakpoint) {
        width: $AjaxCart--width;
    }
}

.AjaxCart-cart_header {
    @extend .clearfix;

    margin-bottom: $AjaxCart-cart_header--spacing;
    padding: 10px ($AjaxCart--gutter_width / 2) 0;
}

.AjaxCart-dismiss {
    @include make-Button--revert(button);
    @include make-Icon--before($Icon-cross, $AjaxCart-dismiss--size);

    float: right;
    font-size: 0;
    margin-top: $AjaxCart-dismiss--misalignment;
    color: $AjaxCart-dismiss--color;
}

.AjaxCart-cart_title {
    font-size: $AjaxCart-cart_title--font_size;
    line-height: $AjaxCart-cart_title--line_height;
    margin: 0;
}

.AjaxCart-divider {
    @include make-Column--gutter(12, min, $Viewport-xs, $AjaxCart--gutter_width);
    margin: 50px ($AjaxCart--gutter_width / 2);
    border-color: #A3A5AD;
}

.AjaxCart-empty_state {
    @include make-Column--gutter(12, min, $Viewport-xs, $AjaxCart--gutter_width);

    position: relative;
    padding: 50px 0;
    text-align: center;

    &:before {
        content: "";

        display: block;

        width: 75px;
        height: 75px;
        margin: 15px auto;

        background: #d8d8d8;
        border-radius: 100%;
    }
}

$AjaxCart-item--spacing: 10px;
$AjaxCart-item--interior_gutter_width: 15px;
$AjaxCart-item--font_family: $Paragraph-font;
$AjaxCart-item--font_weight: $Paragraph-weight;
$AjaxCart-item--font_style: $Paragraph-style;

$AjaxCart-item_title--font_family: $Paragraph-font;
$AjaxCart-item_title--font_weight: $Font-weight--bold;
$AjaxCart-item_title--font_style: $Paragraph-style;
$AjaxCart-item_title--font_size: 14px;
$AjaxCart-item_title--spacing: 0px;

$AjaxCart-item_options--font_size: 12px;
$AjaxCart-item_options--spacing: 10px;

$AjaxCart-item_quantity--height: 30px;
$AjaxCart-item_quantity--width: 75px;
$AjaxCart-item_quantity--font_size: 14px;
$AjaxCart-item_quantity--background_color: #D8D8D8;
$AjaxCart-item_quantity--color: contrast($AjaxCart-item_quantity--background_color, $Font-color--black, $Font-color--white);
$AjaxCart-item_quantity--spacing: 15px;

$AjaxCart-item_quantity_toggle--width: 20px;

$AjaxCart-item_remove--spacing: 15px;
$AjaxCart-item_remove--font_size: 12px;
$AjaxCart-item_remove--color: #D8D8D8;

$AjaxCart-item_price--font_weight: $Font-weight--bold;
$AjaxCart-item_price--font_size: 12px;

.AjaxCart-item {
    @extend .clearfix;
    margin-bottom: $AjaxCart-item--spacing;
}

.AjaxCart-item_image {
    @include make-Column--gutter(4, min, $Viewport-xs, $AjaxCart--gutter_width / 2 + $AjaxCart-item--interior_gutter_width / 2);

    font-size: 0;
    margin-left: ($AjaxCart--gutter_width) / 2;
    margin-right: ($AjaxCart-item--interior_gutter_width) / 2;

    img {
        width: 100%;
        height: auto;
    }
}

.AjaxCart-item_details {
    @include make-Column--gutter(8, min, $Viewport-xs, $AjaxCart--gutter_width / 2 + $AjaxCart-item--interior_gutter_width / 2);
    
    margin-left: ($AjaxCart-item--interior_gutter_width) / 2;
    margin-right: ($AjaxCart--gutter_width) / 2;
}

.AjaxCart-item_title {
    margin: 0 0 $AjaxCart-item_title--spacing;

    font-family: $AjaxCart-item_title--font_family;
    font-weight: $AjaxCart-item_title--font_weight;
    font-style: $AjaxCart-item_title--font_style;
    font-size: $AjaxCart-item_title--font_size;
}

.AjaxCart-item_options {
    margin: 0 0 $AjaxCart-item_options--spacing;
    font-size: $AjaxCart-item_options--font_size;
}

.AjaxCart-item_quantity_selector {
    display: inline-block;
    vertical-align: middle;

    position: relative;
    width: $AjaxCart-item_quantity--width;
    margin: 0 0 $AjaxCart-item_quantity--spacing;

    font-family: $AjaxCart-item--font_family;
    font-weight: $AjaxCart-item--font_weight;
    font-style: $AjaxCart-item--font_style;
    font-size: $AjaxCart-item_quantity--font_size;
}

.AjaxCart-item_quantity {
    width: 100%;
    height: $AjaxCart-item_quantity--height;

    border: 0px solid transparent;

    padding-left: $AjaxCart-item_quantity_toggle--width;
    padding-right: $AjaxCart-item_quantity_toggle--width;

    background-color: $AjaxCart-item_quantity--background_color;
    color: $AjaxCart-item_quantity--color;

    text-align: center;
}

.AjaxCart-item_quantity_toggle {
    @include make-Button--revert(button);

    position: absolute;
    top: 0;
    bottom: 0;
    width: $AjaxCart-item_quantity_toggle--width;

    background: transparent;
    color: $AjaxCart-item_quantity--color;
}

.AjaxCart-item_quantity_toggle--decrease {
    left: 0;
}

.AjaxCart-item_quantity_toggle--increase {
    right: 0;
}

.AjaxCart-item_remove {
    @include make-Button--revert(button);
    @include make-Button--link();

    display: inline-block;
    position: relative;
    vertical-align: middle;

    margin: 0 0 $AjaxCart-item_quantity--spacing $AjaxCart-item_remove--spacing;

    font-family: $AjaxCart-item--font_family;
    font-weight: $AjaxCart-item--font_weight;
    font-style: $AjaxCart-item--font_style;
    font-size: $AjaxCart-item_remove--font_size;
}

.AjaxCart-item_price {
    display: block;
    
    font-weight: $AjaxCart-item_price--font_weight;
    font-size: $AjaxCart-item_price--font_size;
}
$AjaxCart-totals--font_family: $Paragraph-font;
$AjaxCart-totals--font_weight: $Font-weight--bold;
$AjaxCart-totals--font_style: $Paragraph-style;
$AjaxCart-totals--font_size: 12px;
$AjaxCart-totals--line_height: 1.6;

$AjaxCart-total_note--font_family: $Paragraph-font;
$AjaxCart-total_note--font_weight: normal;
$AjaxCart-total_note--font_style: $Paragraph-style;
$AjaxCart-total_note--font_size: 12px;

.AjaxCart-totals {
    @include make-Column--gutter(12, min, $Viewport-xs, $AjaxCart--gutter_width);

    font-family: $AjaxCart-totals--font_family;
    font-weight: $AjaxCart-totals--font_weight;
    font-style: $AjaxCart-totals--font_style;
    font-size: $AjaxCart-totals--font_size;
    line-height: $AjaxCart-totals--line_height;

    th {
        text-align: left;
    }

    td {
        text-align: right;
        position: relative;
    }
}

.AjaxCart-total_note, .AjaxCart-shipping_address_edit_button {
    font-family: $AjaxCart-total_note--font_family;
    font-weight: $AjaxCart-total_note--font_weight;
    font-style: $AjaxCart-total_note--font_style;
    font-size: $AjaxCart-total_note--font_size;
}
.AjaxCart-shipping_address_edit_button {
    @include make-Button--revert(button);
    @include make-Button--link();
}

.is-AjaxCart-shipping_address_edit_button--being_edited {
    display: none;
}

.AjaxCart-shipping_address_form {
